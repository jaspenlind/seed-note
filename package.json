{
  "name": "seed-note",
  "productName": "seed-note",
  "version": "1.0.0",
  "description": "Electron app for building chords",
  "main": ".webpack/main",
  "scripts": {
    "build": "tsc",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "debug": "node --nolazy --inspect-brk=9229 node_modules/jest/bin/jest.js -i",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.{js,ts,jsx,tsx}\" -c",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublishOnly": "npm run test && npm run lint",
    "release:alpha": "npm version prerelease --preid alpha -m \" build: :bookmark:%s\"",
    "release:beta": "npm version prerelease --preid beta -m \"build: :bookmark: %s\"",
    "release:patch": "npm version patch -m \"build: :bookmark: %s\"",
    "release:minor": "npm version minor -m \"build: :bookmark: %s\"",
    "release:major": "npm version major -m \"build: :bookmark: %s\"",
    "test": "jest --coverage -t",
    "test:watch": "jest --watch-all -t",
    "version": "git add -A src"
  },
  "keywords": [
    "music",
    "chords",
    "electron",
    "typescript",
    "sheet-music"
  ],
  "author": {
    "name": "Johan",
    "email": "johan.aspenlind@b3.se"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "seed_note"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/app.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@tonaljs/tonal": "^3.6.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "string-converter": "^1.1.0",
    "tonal-index": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "commitlint": "^8.3.5",
    "coveralls": "^3.0.9",
    "css-loader": "^3.4.2",
    "electron": "7.1.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "node-loader": "^0.6.0",
    "prettier": "^1.19.1",
    "promptly": "^3.0.3",
    "react-svg-loader": "^3.0.3",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  }
}
